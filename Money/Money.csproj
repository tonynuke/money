<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
		<Authors>tonynuke</Authors>
		<RepositoryUrl>https://github.com/tonynuke/Money</RepositoryUrl>
		<Company>tonynuke</Company>
		<PackageId>Tonynuke.Money</PackageId>
		<Version>1.0.0</Version>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Description>Tonynuke.Money - Money for c#.</Description>
		<PackageTags>Money</PackageTags>
	</PropertyGroup>

</Project>
